    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/agabadave/RangeFilter</PackageProjectUrl>
    <RepositoryUrl>https://github.com/agabadave/RangeFilter</RepositoryUrl>
